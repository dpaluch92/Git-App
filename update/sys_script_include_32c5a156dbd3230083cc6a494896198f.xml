        <name>GetRecords</name>
        <script><![CDATA[function GetRecords() { 
	var appNum = [];
	var gr = new GlideRecord('x_284063_git_app_application_table');
	gr.query();
	while (gr.next()) {
		if(gr.inserted_incident.assignment_group.name == 'CAB Approval'){
			appNum.push(gr.number.toString());
		}
	}
	
	// javascript : x_284063_git_app.GetRecords()

	return appNum;
}]]></script>
